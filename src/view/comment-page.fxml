<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="232.0" prefWidth="328.0"
            xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="fxControllers.CommentPage">
    <children>
        <TextField fx:id="commentField" layoutX="40.0" layoutY="77.0" prefHeight="90.0" prefWidth="245.0"
                   promptText="Comment"/>
        <Button fx:id="commentBtn" layoutX="118.0" layoutY="175.0" mnemonicParsing="false" onAction="#createComment"
                text="Add comment"/>
        <TextField fx:id="titleField" promptText="Title" layoutX="40.0" layoutY="36.0" prefHeight="25.0"
                   prefWidth="172.0"/>
    </children>
</AnchorPane>
